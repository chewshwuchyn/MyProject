<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chewshwu.myproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".RegisterActivity" />
        <activity android:name=".MyProject" />
        <activity android:name=".DisplayListView" />
        <activity android:name=".Detail" />
        <activity android:name=".CreateProject" />
        <activity android:name=".TaskListView" />
        <activity android:name=".TaskMain" />
        <activity android:name=".TaskDetailList" />
        <activity android:name=".TaskDetail" />
        <activity android:name=".ToDoList" />
        <activity android:name=".MemberNoList" />
        <activity android:name=".AddProjectMember"/>
        <activity android:name=".ProjectMemberList" />
        <activity android:name=".TaskRList" />
        <activity android:name=".CreateTask"></activity>



    </application>

</manifest>